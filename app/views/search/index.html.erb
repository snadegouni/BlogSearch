<%#<h1>Search#index</h1>
<p>Find me in app/views/search/index.html.erb</p>%>
<h1>Listing blog posts</h1>
 
<table>
  
  <% @posts.each do |post| %>
    <tr>
        <div>
            <%= link_to post.title, post.link , target: :_blank %>
            <br/>
            <br/>
            <%= post.text %>
        </div>
    </tr>
  <% end %>
</table>
